@model List<MovieRentalManagementSystem.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Genre</th>

        </tr>
    </thead>
    <tbody>

        @foreach (var m in Model)
        {

            <tr>
                <td>
                    
                    @Html.ActionLink(m.Name, "Details", new { id = m.Id })
                </td>
                <td> @m.Genre.Name </td>
            </tr>
        }


    </tbody>
</table>

